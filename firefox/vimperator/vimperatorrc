" set options {{{
set maxitems=35
set animations=true
set complete=sltb
set defsearch="google"
set focuscontent
set hintchars="asdfghjueryitbncv"
set incsearch
set ignorecase
set status="input,location,bookmark,history,tabcount,position,ssl"
set newtab="all"
set shell="bash"
set smartcase
set tabnumbers
" TODO :h 'complete'
" set nextpattern="\bnext,^>$,^(>>|»)$,^(>|»),(>|»)$,\bmore\b,next ›"
" set previouspattern="\bprev|previous\b,^<$,^(<<|«)$,^(<|«),(<|«)$,‹ prev"
set nextpattern="next ›,Next >,Next »"
set previouspattern="‹ prev,< Prev,« Prev"
" }}}

" maps {{{
command Selecttab -count tabmove <count>

nnoremap gq    :qmarks<CR>
inoremap <C-c> <Esc>
cnoremap <C-n> <Down>
cnoremap <C-p> <Up>
nnoremap <C-S-F> <PageDown>
nnoremap <C-S-B> <PageUp>

nnoremap <C-j> gt
nnoremap J :Selecttab<CR>:tabmove! -1<CR>
nnoremap > :tabmove<CR>
nnoremap < :tabmove 0<CR>

nnoremap ssa :open amazon<Space>
nnoremap ssd :open dictionary<Space>
nnoremap ssg :open duckduckgo<Space>
nnoremap ssh :history! -max=1<Space>
nnoremap ssi :open images<Space>
nnoremap ssp :open thepiratebay<Space>
nnoremap sst :open thesaurus<Space>
nnoremap ssw :open wikipedia<Space>
nnoremap ssy :open youtube<Space>

nnoremap sa :tabopen amazon<Space>
nnoremap sd :tabopen dictionary<Space>
nnoremap sg :tabopen duckduckgo<Space>
nnoremap sh :history -max=1<Space>
nnoremap si :tabopen images<Space>
nnoremap sp :tabopen thepiratebay<Space>
nnoremap st :tabopen thesaurus<Space>
nnoremap sw :tabopen wikipedia<Space>
nnoremap sy :tabopen youtube<Space>

nnoremap sb :set scrollbar!<CR>

nnoremap <C-f>     /
nnoremap <C-b>     <C-^>
nnoremap <C-l>     :nohlsearch<CR>
nnoremap <C-Space> :styletoggle -name globaldark<CR>

nnoremap <A-l> /<Up><CR>
nnoremap <A-n> :tabmove! +1<CR>
nnoremap <A-p> :tabmove! -1<CR>

nnoremap ,a  :addons<CR>
nnoremap ,e  :extoptions<Space>
nnoremap ,t  :tabopen about:sync-tabs<CR>
nnoremap ,,t :open about:sync-tabs<CR>

nnoremap ,b :sidebar Bookmarks<CR>
nnoremap ,d :sidebar downloads<CR>
nnoremap ,h :sidebar History<CR>
nnoremap ,p :sidebar Preferences<CR>
nnoremap ,q :sbclose<CR>

nnoremap ,g :open google<Space>
nnoremap ,G :tabopen google<Space>

nnoremap <leader>c   :dialog cookies<CR>
nnoremap <leader>d   :dialog savepage<CR>
nnoremap <leader>h   :dialog history<CR>
nnoremap <leader>l   :dialog places<CR>
nnoremap <leader>o   :dialog openfile<CR>
nnoremap <leader>p   :dialog preferences<CR>
nnoremap <leader>s   :dialog searchengines<CR>
nnoremap <leader>t   :dialog customizetoolbar<CR>
nnoremap <leader>u   :dialog pagesource<CR>
nnoremap <leader>x   :dialog cleardata<CR>

nnoremap k <Up>
nnoremap j <Down>
" }}}

" sources {{{
" CSS file added using vimperator `:style`
source ~/dotfiles/firefox/vimperator/globaldark.vimp

hi Hint font-family: DejaVu Sans Mono; letter-spacing:2px; font-size: 12pt; color:#FFFFFF; font-weight:bold; border-color:#3333CC; border-width: 2px; border-style:solid; padding 0px 1px 0px 1px; background: linear-gradient(to bottom left, #000000, #303030); text-transform:uppercase; border-radius: 5px;

hi Filter color: #FF5F00
hi CompItem[selected] background: #331033; color: #FFFFFF;
hi Normal color: #AFAFAF; background: #080808;
hi CompTitle color: #FFAFAF; background: #404040; font-weight:bold;
" }}}

echo "sup"
